{
  "name": "Save as PDF",
  "version": "1.7",
  "manifest_version": 2,
  "description": "Lets you download web pages as PDF in one click.",
  "permissions": [
    "http://pdfcrowd.com/*",
    "tabs"
  ],

 "browser_action": {
     "default_icon": "icons/icon19.png"
 },

 "icons": { "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png" },

 "background" : { "page": "background.html" },
 "options_page": "options.html"
}